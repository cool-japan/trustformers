{
  "name": "@trustformers/svelte",
  "version": "1.0.0",
  "description": "Svelte bindings for TrustformeRS WASM - seamless AI/ML tensor operations in Svelte applications",
  "type": "module",
  "main": "src/svelte/index.js",
  "module": "src/svelte/index.js",
  "svelte": "src/svelte/index.js",
  "exports": {
    ".": {
      "svelte": "./src/svelte/index.js",
      "import": "./src/svelte/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "svelte": "./src/svelte/index.js"
    },
    "./bindings": {
      "import": "./src/svelte_bindings.js"
    },
    "./TrustformersProvider": {
      "svelte": "./src/svelte/TrustformersProvider.svelte"
    },
    "./TensorVisualization": {
      "svelte": "./src/svelte/TensorVisualization.svelte"
    },
    "./TensorOperations": {
      "svelte": "./src/svelte/TensorOperations.svelte"
    }
  },
  "files": [
    "src/svelte/",
    "src/svelte_bindings.js",
    "dist/",
    "types/",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir types",
    "dev": "rollup -c rollup.config.js -w",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/svelte/ src/svelte_bindings.js",
    "lint:fix": "eslint src/svelte/ src/svelte_bindings.js --fix",
    "format": "prettier --write src/svelte/ src/svelte_bindings.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "package": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs": "typedoc --out docs src/svelte/index.js",
    "prepare": "npm run build && npm run build:types"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-svelte-csf": "^4.1.1",
    "@storybook/blocks": "^7.6.7",
    "@storybook/svelte": "^7.6.7",
    "@storybook/sveltekit": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.3",
    "@sveltejs/package": "^2.2.6",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "rollup": "^4.9.6",
    "rollup-plugin-svelte": "^7.1.6",
    "storybook": "^7.6.7",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.3",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "keywords": [
    "svelte",
    "webassembly",
    "wasm",
    "machine-learning",
    "ai",
    "tensor",
    "neural-networks",
    "transformers",
    "webgpu",
    "typescript",
    "reactive",
    "components"
  ],
  "author": {
    "name": "TrustformeRS Team",
    "email": "team@trustformers.dev",
    "url": "https://trustformers.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustformers/trustformers.git",
    "directory": "trustformers-wasm"
  },
  "bugs": {
    "url": "https://github.com/trustformers/trustformers/issues"
  },
  "homepage": "https://trustformers.dev/docs/wasm/svelte",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/trustformers"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false,
  "svelte": "src/svelte/index.js",
  "types": "types/index.d.ts",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "svelte",
      "@typescript-eslint"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "parser": "svelte-eslint-parser",
        "parserOptions": {
          "parser": "@typescript-eslint/parser"
        }
      }
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2022
    },
    "env": {
      "browser": true,
      "es2022": true,
      "node": true
    }
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}